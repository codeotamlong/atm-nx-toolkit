  "cfw": {
    "description": "ATMOSPHERE on MARIKO EMUNAND (Src: https://rentry.co/MarikoEmuNAND)",
    "dl_path": "cfw",
    "sd_path": ".",
    "component": [
      {
        "name": "SX Gear",
        "link": "https://web.archive.org/web/20210217231219/https://sx.xecuter.com/download/SX_Gear_v1.1.zip",
        "isDisabled":true,
        "description": "(Skip this if you use a HWFLY modchip)"
      },
      {
        "name": "Hekate",
        "repo": "CTCaer/hekate",
        "regex": [
          "hekate_ctcaer_.*_Nyx_.*.zip"
        ],
        "isDisabled": false
      },
      {
        "name": "Atmosphere & fusee",
        "repo": "Atmosphere-NX/Atmosphere",
        "regex": [
          "atmosphere-.*-master-.*.zip",
          "fusee.bin"
        ],
        "isDisabled": false
      },
      {
        "name": "SigPatches",
        "link": "https://sigmapatches.coomer.party/sigpatches.zip",
        "isDisabled": false
      }
    ],
    "config": [
      {
        "location": "./bootloader/hekate_ipl.ini",
        "description": "Setup Hekate",
        "content": [
          "[config]",
          "autoboot=0",
          "autoboot_list=0",
          "bootwait=3",
          "backlight=100",
          "autohosoff=0",
          "autonogc=1",
          "updater2p=0",
          "bootprotect=0",
          "",
          "[Atmosphere CFW]",
          "payload=bootloader/payloads/fusee.bin",
          "icon=bootloader/res/icon_payload.bmp",
          "",
          "[Warmboot Error Fix]",
          "fss0=atmosphere/package3",
          "stock=1",
          "emummc_force_disable=1",
          "icon=bootloader/res/icon_switch.bmp"
        ]
      },
      {
        "location": "./exosphere.ini",
        "description":"Setup Exosphere - Hide Serial Number",
        "content": [
          "[exosphere]",
          "debugmode=1",
          "debugmode_user=0",
          "disable_user_exception_handlers=0",
          "enable_user_pmu_access=0",
          "blank_prodinfo_sysmmc=0",
          "blank_prodinfo_emummc=1",
          "allow_writing_to_cal_sysmmc=0",
          "log_port=0",
          "log_baud_rate=115200",
          "log_inverted=0"
        ]
      },
      {
        "location": "./atmosphere/hosts/default.txt",
        "description":"Setup DNS MITM - Block Nintendo Servers",
        "content": [
          "# Block Nintendo Servers",
          "127.0.0.1 *nintendo.*",
          "127.0.0.1 *nintendo-europe.com",
          "127.0.0.1 *nintendoswitch.*",
          "95.216.149.205 *conntest.nintendowifi.net",
          "95.216.149.205 *ctest.cdn.nintendo.net"
        ]
      }
    ]
  },
